<% content_for :title do %>My super sweet dashboard<% end %>
<div class="gridster">
  <ul>
  <li data-row="1" data-col="1" data-sizex="4" data-sizey="1">
    <div data-id="my-table" data-view="Table" data-title="PR Checks"></div>
  </li>

  <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
    <div data-view="Countdown" data-title="Next Code Freeze" data-end="21-Feb-2020 18:00:00"></div>
  </li>

  <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
    <div data-id="current_release" data-view="List" data-unordered="true" data-title="Current Release" style="background-color:#96bf48;"></div>
  </li>

  <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
    <div data-id="total_num_of_tests" data-view="Number" data-title="Total Number of Tests" style="background-color:#96bf48;"></div>
  </li>

  <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
    <div data-id="num_of_manual_tests" data-view="Number" data-title="Total Number of Manual Tests"></div>
  </li>

  </ul>
</div>
